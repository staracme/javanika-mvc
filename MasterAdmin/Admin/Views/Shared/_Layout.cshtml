                        @*<li class="@Html.IsActive("Events")">
                            <a onclick="navigate('events')" href="javascript:void(0);"><i class="fa fa-home"></i>Events </a>
                        </li>*@
                        <li class="@Html.IsActive("BlockSeat")">
