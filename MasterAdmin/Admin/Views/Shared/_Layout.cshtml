                            </a>
                        </li>
                        <!-- <!-- <li><a href="#"><i class="fa fa-suitcase"></i>Vacant </a></li> -->
                        <!--  <li><a href="#"><i class="fa fa-file-image-o"></i>Vacant </a></li>

                        <li><a href="#"><i class="fa fa-newspaper-o"></i>Vacant </a></li>

                        <li><a href="#"><i class="fa fa-user"></i>Vacant </a></li>

                        <li><a href="#"><i class="fa fa-eye"></i>Vacant </a></li>
                        <li><a href="#"><i class="fa fa-laptop"></i>Vacant </a></li> -->
                    </ul>

                </div>
            </div>
        </div>
        <div class="col-sm-10">
            <div class="col-sm-12" style="padding-left:0 !important;">
                <div class="MainTopHeaderWrap">
                    @*<div class="MainHeadLabel1"><div class="LogoSpace">SANOFI INDIA</div></div>*@
                    <div class="MainHeadLabel2"><div class="Namelabel">Welcome <span>@Request.Cookies["Admin"]["FirstName"] @Request.Cookies["Admin"]["LastName"]</span></div></div>
                    @*<div class="TopHeadBtnWrap"><button class="TopHeadBtn">Nischay</button></div>
                        <div class="TopHeadBtnWrap"><button class="TopHeadBtn">Pharmacy</button></div>
                        <div class="TopHeadBtnWrap"><button class="TopHeadBtnActive">Wholesale</button></div>*@
                    <div class="clr"></div>
                </div>
            </div>
            <div class="col-sm-12">
                <div class="MainContentBody">
                    @RenderBody()
                </div>
            </div>
        </div>

    </div>
    <div class="modal fade" id="modalLoader" role="dialog">
        <div id="divLoader" class="my-img-container">
            <div class="loader">
                <div class="lds-spinner"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div>
            </div>
        </div>
        @*<div class="modal-dialog err-pop" style="">
