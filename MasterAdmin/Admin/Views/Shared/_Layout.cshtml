                        </div>
                    </div>
                </div>
                <input id="hdnEventId" type="hidden" value="-1" />
            </div>
        </div>*@
    </div>
</body>
<script type="text/javascript">
    function navigate(menuName) {
        $("#modalLoader").modal('show');
        switch (menuName) {
            case 'myorders':
                window.location.href = '@Url.Action("Index", "MyOrders")';
                break;
            case 'orders':
                window.location.href = '@Url.Action("Index", "Orders")';
                break;
            case 'newevents':
                window.location.href = '@Url.Action("Index","NewEvents")';
                break;
            case 'events':
                window.location.href = '@Url.Action("Index","Events")';
                break;
            case 'block-seat':
                window.location.href = '@Url.Action("Index", "BlockSeat")';
                break;
            case 'coupon-generation':
                window.location.href = '@Url.Action("Index", "CouponGeneration")';
                break;
            case 'export-mis':
                window.location.href = '@Url.Action("ExportMIS", "VOBAEvent")';
                break;
            default:
                window.location.href = '@Url.Action("Index", "Home")';
        }
        @*if (menuName === 'events') {
            var url = '@Url.Action("Index","Events")';
            window.location.href = url;
            /href = "/VOBAEvent / ExportMIS"
        }
        else if (menuName === 'events') {
            var url = '@Url.Action("Index","Events")';
            window.location.href = url;
        }*@
    }
    $(document).ready(function () {

        $("#addEvent").on("click", function () {
            showAddEvent("addEvent");
        });
        $("#listEvent").on("click", function () {
            showAddEvent("listEvent");
        });

    });
</script>
</html>
